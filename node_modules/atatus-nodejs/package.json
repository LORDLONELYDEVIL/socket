{
  "name": "atatus-nodejs",
  "version": "2.0.0",
  "description": "Atatus APM agent for Node.js",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "docs:open": "PREVIEW=1 npm run docs:build",
    "docs:build": "./docs/scripts/build_docs.sh apm-agent-nodejs ./docs ./build",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "lint:commit": "test/lint-commits.sh",
    "coverage": "COVERAGE=true ./test/script/run_tests.sh",
    "coverage:prereport": "npm run coverage",
    "coverage:report": "nyc report --reporter=lcov",
    "test": "./test/script/run_tests.sh",
    "test:cli": "node test/script/cli.js",
    "test:deps": "dependency-check . -i async_hooks -i parseurl -i perf_hooks && dependency-check . --unused --no-dev --entry lib/instrumentation/modules/*",
    "test:tav": "tav --quiet",
    "test:docs": "./test/script/docker/run_docs.sh",
    "test:types": "tsc --project test/types/tsconfig.json && tsc --project test/types/transpile/tsconfig.json && node test/types/transpile/index.js",
    "test:babel": "babel test/babel/src.js --out-file test/babel/out.js && node test/babel/out.js",
    "test:esm": "node --experimental-modules test/esm",
    "local:start": "./test/script/local-deps-start.sh",
    "local:stop": "./test/script/local-deps-stop.sh",
    "docker:start": "docker-compose -f ./test/docker-compose.yml up -d",
    "docker:stop": "docker-compose -f ./test/docker-compose.yml down",
    "docker:clean": "./test/script/docker/cleanup.sh",
    "docker:dev": "docker-compose -f ./dev-utils/docker-compose.yml run --workdir=/agent nodejs-agent"
  },
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "atatus",
    "apm",
    "log",
    "logging",
    "bug",
    "bugs",
    "error",
    "errors",
    "exception",
    "exceptions",
    "catch",
    "monitor",
    "monitoring",
    "alert",
    "alerts",
    "performance",
    "apm",
    "ops",
    "devops",
    "stacktrace",
    "trace",
    "tracing",
    "distributedtracing",
    "distributed-tracing"
  ],
  "homepage": "https://www.atatus.com",
  "dependencies": {
    "after-all-results": "^2.0.0",
    "async-value-promise": "^1.1.1",
    "axios": "^1.6.2",
    "basic-auth": "^2.0.1",
    "console-log-level": "^1.4.0",
    "container-info": "^1.0.1",
    "cookie": "^0.4.0",
    "core-util-is": "^1.0.2",
    "end-of-stream": "^1.4.1",
    "fast-safe-stringify": "^2.0.6",
    "http-headers": "^3.0.2",
    "is-native": "^1.0.1",
    "js-priority-queue": "^0.1.5",
    "measured-reporting": "^1.49.0",
    "monitor-event-loop-delay": "^1.0.0",
    "object-filter-sequence": "^1.0.0",
    "object.entries": "^1.1.0",
    "original-url": "^1.2.3",
    "read-pkg-up": "^4.0.0",
    "redact-secrets": "^1.0.0",
    "relative-microtime": "^2.0.0",
    "require-ancestors": "^1.0.0",
    "require-in-the-middle": "^4.0.0",
    "semver": "^6.1.1",
    "set-cookie-serde": "^1.0.0",
    "sql-summary": "^1.0.1",
    "stackman": "^3.0.2",
    "traceparent": "^1.0.0",
    "unicode-byte-truncate": "^1.0.0"
  },
  "devDependencies": {
    "@apollo/server": "^4.2.2",
    "@aws-sdk/client-s3": "^3.363.0",
    "@aws-sdk/s3-request-presigner": "^3.363.0",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@elastic/elasticsearch": "^8.6.0",
    "@elastic/elasticsearch-canary": "^8.8.0-canary.2",
    "@fastify/formbody": "^7.0.1",
    "@hapi/hapi": "^21.0.0",
    "@koa/router": "^12.0.0",
    "@types/node": "^20.1.0",
    "apollo-server-express": "^3.0.0",
    "aws-sdk": "^2.1400.0",
    "bluebird": "^3.7.2",
    "cassandra-driver": "^4.4.0",
    "connect": "^3.7.0",
    "dependency-check": "^4.1.0",
    "elasticsearch": "^16.7.3",
    "express": "^4.17.1",
    "express-queue": "^0.0.13",
    "fastify": "^4.16.3",
    "finalhandler": "^1.1.2",
    "generic-pool": "^3.7.1",
    "get-port": "^5.1.1",
    "got": "^11.8.5",
    "graphql": "^16.7.1",
    "handlebars": "^4.7.3",
    "https-pem": "^3.0.0",
    "inquirer": "^0.12.0",
    "ioredis": "^5.1.0",
    "is-my-json-valid": "^2.19.0",
    "json-schema-ref-parser": "^6.0.1",
    "knex": "^2.4.2",
    "koa": "^2.11.0",
    "koa-router": "^12.0.0",
    "lambda-local": "^2.0.2",
    "mimic-response": "1.0.0",
    "mkdirp": "^0.5.1",
    "mongodb": "^5.1.0",
    "mongodb-core": "^3.2.7",
    "mysql": "^2.18.1",
    "mysql2": "^3.2.4",
    "ndjson": "^1.5.0",
    "nyc": "^15.0.0",
    "once": "^1.4.0",
    "p-finally": "^1.0.0",
    "pg": "^8.7.1",
    "pug": "^3.0.1",
    "redis": "^4.3.0",
    "restify": "^11.0.0",
    "restify-clients": "^2.6.6",
    "rimraf": "^3.0.2",
    "send": "^0.17.0",
    "standard": "^12.0.1",
    "tape": "^5.0.0",
    "tedious": "^16.1.0",
    "test-all-versions": "^4.1.1",
    "thunky": "^1.1.0",
    "typescript": "^5.0.2",
    "untildify": "^4.0.0",
    "util.promisify": "^1.0.0",
    "wait-on": "^7.0.1",
    "ws": "^7.2.1"
  },
  "greenkeeper": {
    "ignore": [
      "inquirer"
    ]
  },
  "standard": {
    "ignore": [
      "/test/sourcemaps/fixtures/lib"
    ]
  },
  "commitlint": {
    "extends": [
      "squash-pr",
      "@commitlint/config-conventional"
    ]
  }
}
